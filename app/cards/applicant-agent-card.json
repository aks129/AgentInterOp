{
  "protocolVersion": "0.2.9",
  "preferredTransport": "JSONRPC",
  "name": "Applicant Agent",
  "description": "FHIR-enabled applicant agent for healthcare benefits eligibility processing",
  "role": "applicant",
  "capabilities": {
    "streaming": true,
    "protocols": ["A2A", "MCP"],
    "fhir": true
  },
  "skills": [
    {
      "name": "bcse_eligibility_application",
      "description": "Process FHIR patient data and generate eligibility applications for BCS-E screening benefits",
      "type": "discovery",
      "scenario": "healthcare_eligibility",
      "inputs": ["patient_data", "requirements"],
      "outputs": ["questionnaire_response", "fhir_bundle"],
      "a2a": {
        "config64": "ZGVtby1jb25maWc="
      }
    }
  ],
  "methods": [
    "load_patient",
    "answer_requirements", 
    "make_bundle_for_decision"
  ],
  "supported_formats": [
    "application/fhir+json",
    "application/json"
  ]
}
