{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    {
      "src": "/api/bridge/.*",
      "dest": "api/index.py",
      "headers": {
        "Cache-Control": "no-cache",
        "X-Accel-Buffering": "no"
      }
    },
    {
      "src": "/.well-known/agent-card.json",
      "dest": "api/index.py",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "framework": null
}